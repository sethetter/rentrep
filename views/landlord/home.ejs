<div class="main-left">
	<div class="main-left-inner">
		<%- partial('profile.ejs') %>
	</div>
</div>
<div class="main-right">
	<div class="main-right-inner gallery-listing clearfix">
		<div class="listing-gallery-head clearfix">
			<h2 class="listing-title">My Properties</h2>
			<form class="form-listing-search">
				<div class="form-group">
					<label class="form-label" for="search">Address</label>
					<input type="text" class="form-control" name="search" id="search"/>
					<button class="btn"><span class="icon btn-go">Search</span></button>
				</div>
			</form>
		</div>
		<div class="grid grid-listing">
		<% var images = [
			'https://cldup.com/7D3HEG0QA3.jpg',
			'https://cldup.com/CEvgtxsHik.jpg',
			'https://cldup.com/xb-y9IKbtg.png',
			'https://cldup.com/NVg0YLdDym.jpg',
			'https://cldup.com/PcLIIlyTMd.jpg',
			'https://cldup.com/DFq54qi9BA.png',
			'https://cldup.com/hBhSowK9jf.png',
			'https://cldup.com/5-yvnC1PaY.jpg',
		] %>
     	<% properties.forEach(function(property) { %>
        <div class="unit-listing unit4-md">
          <a href="/property/<%= property.id %>" class="item-listing">
            <span class="item-listing-head item-listing-active">
              <span class="listing-alert"><span class="list-alert-icon">!</span></span>
              <div class="item-listing-image-container">
              	<img class="item-listing-image" src="<%= images[Math.floor(Math.random() * images.length)] %>" alt="" />
              </div>
            </span>
            <span class="item-listing-body clearfix">
              <span class="listing-address">
                <%= property.streetAddress %>
                <br />
                <%= property.city %>, <%= property.state %>, <%= property.zip %></span>
              <span class="listing-price">$<%= property.price %></span>
            </span>
          </a><!-- .item-listing -->
        </div><!-- .unit-listing -->
      <% }) %>

		</div><!-- .grid-listing -->
	</div>
</div>
