<form class="form-register">
	<div class="form-group">
		<lable for="username" class="form-lable">Username</label>
		<input type="text" id="username" class="form-control" name="username" required />
	</div>
	<div class="form-group">
		<lable for="email" class="form-lable">Email</label>
		<input type="email" id="email" class="form-control" name="email" required />
	</div>
	<div class="form-group">
		<lable for="firstname" class="form-lable">First Name</label>
		<input type="text" id="firstname" class="form-control" name="firstname" required />
	</div>
	<div class="form-group">
		<lable for="lastname" class="form-lable">Last Name</label>
		<input type="text" id="lastname" class="form-control" name="lastname" required />
	</div>
	<div class="form-group">
		<lable for="password" class="form-lable">Password</label>
		<input type="password" id="password" class="form-control" name="password" required />
	</div>
</form>